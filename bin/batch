#!/usr/bin/env node

const {
  saveProposals,
  currentHeight,
  reportVotingResult,
  confirmVotingResult,
} = require('../lib/sebak');


async function tasks() {
  console.log('currentHeight:', await currentHeight());
  await saveProposals();
  await reportVotingResult();
  await confirmVotingResult();
}


tasks().then((res) => ({}), (err) => {
  console.log(err);
});
